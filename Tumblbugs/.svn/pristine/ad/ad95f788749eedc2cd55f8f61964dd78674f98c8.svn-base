package com.tumblbugs.vo;

public class EmailVO {
	public static String fromEmailAddr = "tumblbugs.service@gmail.com";
	String toEmailAddr, title, content;

	public static String getFromEmailAddr() {
		return fromEmailAddr;
	}

	public static void setFromEmailAddr(String fromEmailAddr) {
		EmailVO.fromEmailAddr = fromEmailAddr;
	}

	

	public String getToEmailAddr() {
		return toEmailAddr;
	}

	public void setToEmailAddr(String toEmailAddr) {
		this.toEmailAddr = toEmailAddr;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}
	
}
