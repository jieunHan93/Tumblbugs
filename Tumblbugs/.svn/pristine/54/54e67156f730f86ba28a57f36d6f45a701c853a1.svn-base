	/*** 선물 선택하기 ***/
	form#gift_select_form {
		width: 680px;
		margin: auto;
		border: 1px solid #ebe8e8;
	}
	form#gift_select_form label {
		width: 100%;
		margin: 0px;
		border-bottom: 1px solid #ebe8e8;
		cursor: pointer;
		padding: 20px 0px;
		line-height: 1.75;
	}
	form#gift_select_form label:hover {
		background-color: #fafafa;
	}
	form#gift_select_form label.disabled {
		cursor: default;
		background-color: #ebeae5;
		opacity: 0.7;
	}
	.gift_select_yn[type="checkbox"] {display: none;}
	.gift_select .gift_price {
		float: left;
		width: 160px;
		font-size: 15pt;
		font-weight: bold;
		text-align: center;
	}
	.gift_select .gift_detail {
		float: right;
		text-align: left;
		width: 510px;
	}
	.gift_title, .select_cancel {font-size: 11.5pt; display: inline-block;}
	.select_cancel {
		display: none;
		float: right;
		border: 1px solid #1e90ff;
		background-color: white;
		color: #1e90ff;
		width: 70px;
		text-align: center;
		margin-right: 20px;
	}
	.gift_select .items {
		font-size: 11pt;
		list-style-type: disc;
		list-style-position: inside;
		margin-left: 10px;
	}
	.gift_select .expected_delivery_day,
	.gift_select .gift_quantity,
	.gift_select .gift_option {font-size: 10.5pt;}
	.gift_select .gift_quantity {margin-top: 10px;}
	.gift_select .gift_quantity_and_option {display: none;}
	
	/* 수량 선택 */
	.gift_select .option_label {
		display: inline-block;
		width: 40px;
	}
	.gift_select .gift_quantity input#quantity {
		padding: 2px 5px;
		width: 35px; height: 25px;
		text-align: center;
		background-color: white;
		border: none;
		border-left: 1px solid lightgray;
		border-right: 1px solid lightgray;
	}
	.quantity_box {
		display: inline-block;
		border: 1px solid lightgray;
	}
	#quantity_minus, #quantity_plus {
		padding: 0px 5px;
	}
	
	/* 옵션 선택 */
	.option_box {
		float: right;
	}
	.gift_select .gift_option {
		overflow: auto;
		padding-top: 5px;
	}
	.gift_select .gift_option>div {display: inline-block;}
	.gift_select .gift_option>.option_label {float: left; margin-top: 5px;}
	.gift_select .gift_option .select_option_box {
		width: 467px;
		overflow: auto;
	}
	.select_option_box select {
		width: 95%;
		display: block;
		border: 1px solid lightgray;
		padding: 2px 5px;
		margin: 5px 0px;
	}
	.write_option_box {
		width: 467px;
		margin: 5px 0px;
		overflow: auto;
	}
	.write_option_box #optionType3_message {
		font-size: 10pt;
	}
	.write_option_box input {
		width: 95%;
		border: 1px solid lightgray;
		padding: 2px 5px;
	}
	
	/* 추가 후원금 입력 */
	#extra_funding input {
		text-align: right;
		width: 180px;
		padding: 4px 8px;
		border: 1px solid lightgray;
		border-radius: 4px;
	}
	
	/* confirm */
	.gift_select .confirm {
		padding: 40px 0px;
		font-weight: bold;
	}
	
	/* 변경 버튼 */
	#btn_gift_edit {
		width: 120px; height: 40px;
		border: none;
		border-radius: 5px;
		font-size: 11pt; font-weight: bold;
		background-color: #1e90ff;
		color: white;
	}
	#btn_gift_edit:hover {
		background-color: #1d85ea;
	}

	/*** 결제 수단 변경하기 ***/
	.payment_edit {
		font-size: 11pt;
	}
	#my_payment {padding-top: 30px;}
	#my_payment div {
		border: 1px solid lightgray;
		width: 400px;
		padding: 12px 20px;
		text-align: left;
		border-radius: 5px;
		background-color: #f7f7f7;
		box-shadow: 1px 1px #dcdcdc;
		cursor: pointer;
		margin: auto;
		margin-bottom: 10px;
	}
	#my_payment div i {margin-right: 10px;}
	#my_payment a {
		text-decoration: none;
		color: #676363;
	}

	/*** 취소하기 ***/
	.cancel_box {
		margin: auto;
		background-color: #f7f7f7;
		width: 500px;
		padding: 50px 0px;
		margin-bottom: 20px;
		font-size: 11pt;
		font-weight: bold;
	}
	
	.funding_cancel button {
		width: 100px; height: 33px;
		font-size: 10.5pt;
		border: none;
		margin: 10px;
		outline: none;
	}
	#btn_funding_cancel {
		background-color: #1e90ff;
		border-radius: 4px;
		color:white;
	}
	#btn_funding_cancel:hover {
		background-color: #1d85ea;
	}
	#btn_go_funding_detail {
		background-color: white;
	}
	#btn_go_funding_detail:hover {
		color: #fa6462;
	}


