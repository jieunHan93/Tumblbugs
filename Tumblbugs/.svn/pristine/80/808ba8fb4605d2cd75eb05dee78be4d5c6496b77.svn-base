*{
		font-family: SpoqaHanSans;
	}
	#project_start_upload section{
		width:100%;
		text-align:center;
		margin:auto;
	}
	#project_start_upload #start_upload_header{
		border-bottom: 1px solid rgb(239,239,239);
		padding:5px 0px 5px 0px;
		margin:auto;
		text-align:center;
	}
	#project_start_upload #start_upload_header>div{
		width:1080px;
		display:inline-block;
	}
	#project_start_upload #start_upload_header>div>div{
		display:inline-block;
		padding:10px 20px 10px 20px;
		font-size:10.5pt;
		color:#1e90ff;
		font-weight:550;
		letter-spacing:0.3px;
	}
	#project_start_upload #start_upload_header>div>div:nth-child(2){
		font-weight:normal;
		font-size:10pt;
		font-style:italic;
		letter-spacing:-1px;
		font-weight:450;
	}
	#project_start_upload #start_upload_header>div>div:first-child{
		float:left;
	}
	#project_start_upload #start_upload_header>div>div:last-child{
		float:right;
	}
	#project_start_upload #start_upload_header_detail{
		width:100%;
		height:316px;
/* 		border:1px solid red; */
	}
	#project_start_upload #upload_title{
		width:1080px;
		height:140px;
		text-align:center;
		display:inline-block;
		position:relative;
	}
	#project_start_upload #upload_title>div{
		width:100%;
		position:absolute;
		top:40%;
	}
	#project_start_upload #upload_title span{
		border:1px solid gray;
		border-radius:4px;
		padding:4px 10px 4px 10px;
		color:gray;
		font-size:8.5pt;
	} 
	#project_start_upload #upload_my_title{
		font-size:15pt;
		padding-left:8px;
		font-weight:530;
		display:inline-block;
		position:relative;
		top:2px;
	}
	#project_start_upload #start_upload_header_detail>div:nth-child(2){
		display:inline-block;
		background:rgb(223,240,255);
		border-radius:3px;
		width:1080px;
		padding:14px 0px 14px 0px;
		margin-bottom:6px;
		font-size:10.5pt;
		color:rgb(54,112,241);
	}
	#project_start_upload #start_upload_header_detail>div:nth-child(3){
		display:inline-block;
		background:rgb(247,247,247);
		border-radius:3px;
		width:1080px;
		padding:14px 0px 14px 10px;
		margin-bottom:0px;
		font-size:10.5pt;
		color:rgb(84,84,84);
	}
	#project_start_upload #upload_menu{
		display:inline-block;
		width:1080px;
		/* border:1px solid purple; */
		position:relative;
		top:17px;
	}
	#project_start_upload #upload_menu label{
		padding:13px 25px 13px 25px;
		font-size:10.5pt;
		font-weight:bold;
		border-radius:4px;
		float:left;
		border:1px solid white;
		border-bottom:none;
		letter-spacing:1px;
		cursor:pointer;
	}
	#project_start_upload #upload_menu input[type=radio]{
		display:none;
	}
	#project_start_upload textarea{
		width:92%;
		height:400px;
		border:2px solid rgb(204,218,252);
		border-radius:4px;
		position:relative;
		bottom:10px; 
		line-height:180%;
		resize:none;
	}
	
	#project_start_upload #upload_menu>div{
		float:right;
		display:inline-block;
		margin:0px;
		position:relative;
	}
	#project_start_upload #upload_menu>div>button:first-child{
		padding:6px 25px 6px 25px;
		font-size:10pt;
		font-weight:bold;
		border-radius:4px;
		border:2px solid gray;
		color:gray;
		background:none;
		margin:0px;
	}
	#project_start_upload #upload_menu>div>button:nth-child(2){
		padding:6px 25px 6px 25px;
		font-size:10pt;
		font-weight:bold;
		border-radius:4px;
		border:2px solid rgb(252,113,109);
		color:rgb(252,113,109);
		background:none;
		margin:0px;
	}
	#project_start_upload #upload_menu>div>button:last-child{
		padding:6px 25px 6px 25px;
		font-size:10pt;
		font-weight:bold;
		border-radius:4px;
		border:2px solid #1e90ff;
		color:#1e90ff;
		background:none;
		margin:0px;
	}
	#project_start_upload section:nth-child(2){
		width:100%;
		background:rgb(247,247,247);
		position:relative;
		bottom:10px;
		margin-bottom:0px;
		min-height:650px;
	}
	
