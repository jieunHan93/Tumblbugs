/***
* index
* 한지은
***/
section.main_content_section{
			width:1080px;	
			margin:auto;
}
    .swiper-container {
      width: 100%;
      /* height: 100%; */
    }
    .swiper-slide {
      text-align: center;
      font-size: 18px;
      background: #fff;
      /* Center slide text vertically */
      display: -webkit-box;
      display: -ms-flexbox;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: center;
      -ms-flex-pack: center;
      -webkit-justify-content: center;
      justify-content: center;
      -webkit-box-align: center;
      -ms-flex-align: center;
      -webkit-align-items: center;
      align-items: center;
    }
    div#main_header .swiper-container{
    	width:100%;
    	height:450px;
    }
    div#main_header div.swiper-pagination{
    	vertical-align: bottom;
    }
    div#main_slide1.swiper-slide{ background-image: url("http://localhost:9090/tumblbugs/images/hero_aldif.jpg"); }
    div#main_slide2.swiper-slide{ background-image: url("http://localhost:9090/tumblbugs/images/hero_delta_time.jpg"); }
    div#main_slide3.swiper-slide{ background-image: url("http://localhost:9090/tumblbugs/images/hero_lunox_mod1.jpg"); }
    div#main_slide4.swiper-slide{ background-image: url("http://localhost:9090/tumblbugs/images/hero_nana.jpg"); }
    div#main_slide5.swiper-slide{ background-image: url("http://localhost:9090/tumblbugs/images/hero_planner.jpg"); }
    div.swiper-slide>a {
    	width:100%;
    	height:450px;
    }
    h1.swiper-title, div.swiper-subtitle{
    	color:white;
    }
    h1.swiper-title {
    	font-size: 45px;
    	line-height: 55px;
    	font-weight:bold;
    }
    div.swiper-subtitle{
    	padding-top:20px;
    	font-size: 20px;
    	font-weight:bold;
    }
    div#swiper-content{
    	display: inline-block;
    	position:absolute;
    	bottom:0px;
    	left: 430px;
    	padding-bottom:40px;
    }
    div.swiper-slide{
		text-align: left;
    }
    div.slick-card{
    	width:242px;	height: 315px;
    	border:1px solid rgba(10,10,10,0.1);
    	border-radius:5px;
    	overflow: hidden;
    	transition: box-shadow .3s;
    	margin:10px;
    }
    div#main_content>section{
    	padding:40px 0px 0px 0px;
    }
    div#main_content_title1 {
    	font-size:20px;
    	font-weight: bold;
    	padding-left:10px;
    	padding-bottom:10px;
    	display: inline-block;
    }
  	section.main_content_section>div{
  		margin-left:8px;
  	}
  	
  	section.main_content_section .slick-slide img#product_img{
  		width: 245px;
    	height: 180px;
  	}
  	div.slick-card>a{
  		text-decoration: none !important;
  	}
  	div.slick-card-content-title{
  		height:70px;
  	}
  	div.slick-card-content-title>h1{
  		margin-right:15px;
  		margin-left:15px;	margin-top:15px;
  		color:black;
  		font-size: 14px;
  		font-weight: bold;
  		word-break: keep-all;
  	}
  	div.slick-card-content-title>p{
  		margin-left:15px;
  		color:black;
  		font-size: 12px;
  	}
  	div.slick-card-content-others>div>i{
  		color:#767676;
  		font-size: 12px;
  	}
  	div.slick-card-content-others div{
  		display: inline-block;
  		color:black;
  		/* border:1px solid pink; */
  	}
  	div.slick-card-content-others div:nth-child(1) span,
  	div.slick-card-content-others div:nth-child(2) span{
		font-size: 12px;
	}
	div.slick-card-content-others div:nth-child(1) img {
		display: inline-block;
		vertical-align: middle;
	}
	div.slick-card-content-others div:nth-child(1){
		width:100px;
		margin-left:15px;
	}
  	div.slick-card-content-others div:nth-child(2) {
  		float:right;
  		margin-right:15px;
  	}
  	div.progress { 
  		margin: 0px 10px;
  		margin-bottom:10px;
  		height:2px;
  	}
  	div#main_content_title1>a{
  		text-decoration: none;
  		color:black;
  	}
  	div#main_content_title1>a>svg{
  		overflow: hidden;
    	padding-top: 2px;
    	vertical-align: baseline;
  	}
  	section.main_content_section div.slick-card:hover {
  		box-shadow: 0 0 11px rgba(33,33,33,.2); 
	}
	div.progress-bar {
		background-color: #1e90ff;
	}
	div#main_colection_banner{
		height: 240px;
		background-color: #fafafa;
		padding:30px 0px;
		margin-top:40px;
	}
	div#main_banner_title{
		font-size:20px;
		font-weight: bold;
		padding-left:10px;
	}
	div.main_banner_card{
		width:504px;
		margin:10px 10px;
		height:124px;
		border: 1px solid rgba(10,10,10,0.1);
		transition: box-shadow .3s;
	}
	div#main_colection_banner div.multiple-items {
		margin : 5px 5px;
	}
	
	div.main_banner_card>a{
  		text-decoration: none !important;
  		color:black;
  	}
  	div.main_banner_card_img{
  		width:190px;
  		height:122px;
  		overflow: hidden;
  		float:right;
  	}
  	div.main_banner_card_img>img{
  		width:100%;
  	}
  	div.main_banner_card a>span {
  		clear:both;
  	}
  	span.main_banner_card_title>div{
  		margin:18px;
  		font-size: 16px;
  		font-weight: bold;
  	}
  	span.main_banner_card_count{
  		margin:20px;
  		font-size: 12px;
  		position: absolute;
  	}
  	span.main_banner_card_count>span{
  		color:#1e90ff;
  	}
  	div.main_banner_card_space{
  		width:20px;
  		height:2px;
  		background-color: black;
  		position:absolute;
  		margin-left:20px;
  	}
  	div.main_banner_card:hover {
  		box-shadow: 0 0 11px rgba(33,33,33,.2); 
	}
	div#main_banner_bot{
		background-image: url("http://localhost:9090/tumblbugs/images/banner-bot-background.png");
		opacity: 0.5px;
		background-color: #9945d1;
		height:100px;
		background-position: center center;
	}
	a#main_back_color{
		text-decoration: none;
		color:white;
		margin-top: 20px;
	}
	div#main_banner_bot h2,
	div#main_banner_bot p{
		text-align: center;
	}
	div#main_banner_bot h2 {
		font-weight: bold;
		margin-bottom:2px;
	}
	div#main_banner_bot div{
		border: 1px solid rgba(0,0,0,0);
		position: relative;
   		top: 15px;
	}
	div.multiple-items button, div.multiple-items button:hover, div.multiple-items button:before{
		width: 18px;
		background-color: #ccc;
		border-radius: 50%;
	}
	section#main_title4{
		margin-bottom:60px;
	}
button.top {
