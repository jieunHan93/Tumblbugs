body {/* width: 1080px; margin: auto; */}
/**
* footer style
*/
footer{border-top:1px solid rgba(10,10,10,0.1); }
#footer_div {width: 1080px; margin: auto; color: #757575; padding: 60px 0px 80px 0px;}
#footer_div>div:first-child {height: 160px; vertical-align: top;}
footer a {text-decoration: none; color: #757575;}
footer table a:hover {color: black; text-decoration: none;}
footer table {
	width: 350px;
	float: left;
	font-size: 12pt; font-weight: bold;
}
footer td {padding: 3px 0px 3px 10px;}
#footer_div>div>div {padding: 10px;}
#footer_div>div div {width: 300px; float: right;}
#footer_div div:nth-child(2)>div:first-child {text-align: right; padding-right: 0px;}
footer a.icon {
	 display: inline-block;
	 width: 21px; height: 21px;
	 margin: 0px 0px 15px 15px;
}
footer a.icon>img {width: 20px; height: 21px;}
footer a#kakaotalk>img {width: 21px; height: 18px;}
footer a#facebook>img {width: 12px;}
footer a#naver>img {width: 19px; height: 20px;}
#footer_div #footer_right {text-align: right;}
#footer_div a#goHelpCenter {
	display: inline-block;
	font-size: 10pt;
	width: 150px; height: 35px;
	text-align: center; vertical-align: center;
	border: 2px solid #dddddd;
	border-radius: 5px;
	background-color: white;
	padding: 5px;
}
#footer_div a#goHelpCenter:hover {color: #757575; text-decoration: none; border: 2px solid #bcbcbc;}
#footer_div>p {font-size: 10pt; line-height: 1.8; margin-bottom: 40px; padding: 0px 10px 0px 10px; width: 1085px;}
#footer_div>div:last-child>span {display: inline-block; padding: 0px 10px 0px 10px;}
#footer_div>div:last-child>span:first-child {float: left; font-size: 9pt; width: 870px;}
#footer_div>div:last-child>span:last-child {float: right; font-size: 9pt; width: 160px; text-align: right;}

/***
* header
* 한지은
***/
header{border-bottom:1px solid rgba(10,10,10,0.1); box-shadow: 0px 2px 2px rgba(33,33,33,.05); }
div#header_nav {
	width:1080px;
	margin:auto;
	text-align: center;
	height:65px;
	font-size:14px;
}
div#header_nav_left,
div#header_nav_center,
div#header_nav_right {
	display: inline-block;
	vertical-align: middle;
	text-align: center;
	position:relative;
	top:23px;
	font-weight:bold;
}
div#header_nav_left {
	float:left;
	/* width:265px; */
}
div#header_nav_right{
	float:right;
	position: absolute;
 	right: 420px;
}
div#header_nav_center {
	clear:both;
	position:absolute;
	left: 875px;
	top: 15px;
/* 	width:350px;
	margin-top: -10px;
    margin-left: -85px; */
}
div#header_nav button{
	border: none;
	background-color: transparent;
	cursor:pointer;
	font-size:14px;
	font-weight:bold;
	outline: none;
}
div#header_nav_right a,
div#header_nav_center a,
div#header_nav_left a {
	cursor:pointer;
	color : black;
	text-decoration: none;
}
div#header_nav_left button {
	padding-right: 20px;
}

div#header_nav_right a{
	padding-right: 60px;
	padding-left: 20px;
}
div#user_icon {
	border:1px solid #ddd;
	border-radius: 50%;
	overflow: hidden;
	display: inline-block;
	position:absolute;
	top:-7px; 	right:10px;
	width:38px;
	height:38px;
    background-image: url("http://localhost:9090/tumblbugs/images/kurby.gif");
    background-size: cover;
    background-position: 50% 50%;
}
div#header_nav_center>a>img{
	width: 150px;
	/* position: absolute;
	top: -10px;
	left: -40px; */
}
div#header_nav>button{
	background-image: url("http://localhost:9090/tumblbugs/images/icon_search.png");
	background-size: 20px;
	background-repeat: no-repeat;
	background-position: center;
	width: 20px;
	height: 20px;
	background-color: transparent;
	border: transparent;
	position:absolute;	    
    right: 620px;
 	top: 23px;
	/* position:relative;	    
	right: -140px;
	top: 23px; */
}
div#header_nav_left>button>img{
	height:17px;
	padding-left: 20px;
	padding-right: 5px;
	position:relative;	top:-2px;
}
div#header_nav_right>button{
	/* position: absolute;
    width: 130px;
    right: 0px; */
    float: right;
}

button.top {
	position: fixed;
	left: 95%;
	bottom: 30px;
	display: none;
	background-image:url("http://localhost:9090/tumblbugs/images/icon-top.png");
	background-size:60px;
	width: 60px;
	height:60px;
	border-radius: 50%;
	border:1px solid #ccc;
	opacity: 0.9;
	outline: none !important;
	z-index: 1;
}

