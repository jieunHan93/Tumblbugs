		div#mypage_payment_main_container{
			width:1080px;
			margin:auto;
			min-height:680px;
			padding:40px 0px;
		}
		div#payment_editor{
			width:700px;
			display: inline-block;
		}
		div#payment_faq{
			width:300px;
			float:right;
			display: inline-block;
			margin-right:20px;
			border-radius: 5px;
		}
		div#payment_nav{
			margin-left:20px;
			margin-right:20px;
		}
		div#payment_nav>button:nth-child(2) {
			float:right;
		}
		div#payment_nav>button {
			border:transparent;
			background-color: transparent;
			font-size:20px;
			color:#1e90ff;
			outline: none;
		}
		div#payment_nav>button:hover{
			color:#1d85ea;
		}
		div#registed_payment_list,
		div#payment_regist_editor {
			border-top:1px solid #ebe8e8;
			margin-top:10px;
			width:700px;
			min-height:500px;
		}
		div#registed_payment_list>div{
			width:660px;
			margin:10px 0px 10px 20px;
			height:50px;
			border-radius:5px;
			background-color: #ebe8e8;
		}
		div#registed_payment_list>div:hover{
			box-shadow: 2px 2px 4px rgba(33,33,33,.2); 
		}
		div#registed_payment_list i {
			font-size:22px;
			margin-left:15px;
			margin-top:15px;
		}
		div#registed_payment_list>div>button {
			float:right;
			margin-right:15px;
			border:transparent;
			background-color: transparent;
			font-size:22px;
			outline: none;
		}
		div#registed_payment_list>div>button:hover{
			color:#1e90ff;
		}
		div#registed_payment_list>div>span {
			padding-left:10px;
			font-size: 17px;
		}
		div#payment_faq>div:first-child {
			font-size:17px;
			font-weight:bold;
			color:#676363;
			border-bottom: 1px solid #ebe8e8;
			padding-bottom:8px;
		}
		div.payment_faq_article>div{
			padding:13px 0px 5px 0px;
		}
		div.payment_faq_article>div i{
			font-size:12px;
			color:gray;
			position: relative;
    		top: -2px;
    		margin-right:5px;
		}
		div.payment_faq_title>span{
			font-size:14px;
			font-weight: bold;
		}
		div.payment_faq_content{
			margin-left: 14px;
		    margin-top: 5px;
		    font-size: 14px;
		    color:#676363;
		}
		div.payment_faq_content a{
			text-decoration: none;
			outline: none;
			color: #1e90ff;
		}
		div#payment_regist_select button{
			border:1px solid rgba(10,10,10,0.1);
			background-color: white;
			color:#555555;
			padding: 3px 7px;
			margin-right:5px;
			font-size: 15px;
			width:118px;
			text-align: left;
		}
		div#payment_regist_select button.dropdown-toggle::after{
			float: right;
    		margin-top: 8px;
		}
		.btn-primary:not(:disabled):not(.disabled).active:focus, 
		.btn-primary:not(:disabled):not(.disabled):active:focus, 
		.show>.btn-primary.dropdown-toggle:focus,
		.btn-primary.focus, .btn-primary:focus{
			box-shadow: none !important;
		}
		div#payment_regist_select {
			margin-top:10px;
			padding-left: 20px;
		}
		div#payment_regist_select div.dropdown-menu{
			min-width: 30px;
    		max-width: 118px;
    		margin-top:10px;
		}
		div#payment_regist_select div.dropdown-item{
			padding-left: 10px;
			font-size:15px;
		}
		div#payment_regist_editor>span{
		    display: inline-block;	
			margin:10px 0px 0px 20px;
			color: #645353;
		    font-weight: bold;
		    font-size: 17px;
		}
		div#payment_regist_credit,
		div#payment_regist_account{
			background-color: #ebeae5;
   			border : 1px solid #e7e5e5;
   			color:#676363;
   			width:600px;
   			/* height:400px; */
   			margin-left:20px;
   			margin-top:10px;
   			border-radius: 5px;
		}
		div.payment_regist_space{
			border:3px solid #1e90ff;
			border-radius: 5px 5px 0px 0px;
		}
		div.payment_regist_input{
			margin-left:20px;
			margin-top:20px;
		}
		div.payment_regist_input>label{
			display: block;
			font-size:15px;
			font-weight: 550;
		}
		span.payment_regist_guide{
			margin-top:5px;
			display: block;
			width:450px;
			font-size:13px;
		}
		div#payment_regist_credit input,
		div#payment_regist_account input{
			width:250px;
			border:1px solid #ebe8e8;
			border-radius: 5px;
			padding:3px 6px;
		}
		div#payment_regist_account select{
			border:1px solid #ebe8e8;
			border-radius: 5px;
			padding:3px 6px;
		}
		div#payment_regist_credit div:last-child,
		div#payment_regist_account div:last-child{
			padding-bottom:40px;
		}
		div#payment_regist_credit>div:nth-child(2)>input.form-control {
			width:60px;
			display:inline-block;
		}
		button#payment_regist_submit,
		button#payment_regist_reset{
			padding:10px 40px;
			border:1px solid #1e90ff;
			background-color: #1e90ff;
			border-radius: 5px;
			color:white;
			margin-top:15px;
			outline:none;
			margin-right: 10px;
		}
		button#payment_regist_submit {
			margin-left:170px;
		}
		button#profile_editor_submit:hover,
		button#payment_regist_reset:hover{
			background-color: #1d85ea;
		}
		
/**
	funding_history(내 후원현황) -----------------------------------------------------------------------------------------------------
	조수윤 --------------------------------------------------------------------------------------------------------------------------
**/
	/* 페이지 상단부 - 프로필 이미지, OOO님의 후원 현황 */
	#funding_history section.page_title {
		text-align: center;
		padding: 50px 0px 50px 0px;
	}
	#funding_history section.page_title span {
		display: inline-block;
		width: 105px; height: 105px;
		border-radius: 105px;
		overflow: hidden;
	}
	#funding_history section.page_title img {
		width: 105px;
	}
	#funding_history section.page_title div {
		padding: 10px 0px 10px 0px;
		font-size: 13.5pt;
		font-weight: bold;
	}
	
	/**/
	#funding_history section.page_content {
		background-color: #f6f5f5;
		min-height: 680px;
	}
	#funding_history article {
		width: 1080px;
		margin: auto;
	}
	#funding_history li {list-style-type: none;}
	
	
	/* 탭 */
	#funding_history article#page_content_tab {
		padding: 20px 0px 20px 0px;
	}
	#funding_history #page_content_tab nav,
	#funding_history #page_content_tab li,
	#funding_history .search {
		display: inline-block;
		font-size: 10.5pt;
	}
	#funding_history #page_content_tab li {
		border-radius: 4px;
		padding: 8px 10px 8px 10px;
		margin-right: 8px;
		cursor: pointer;
		/* background-color: #eeeded; */
	}
	#funding_history #page_content_tab li:hover {
		background-color: rgba(0,0,0,.05);
	}
	#funding_history #page_content_tab li span#list_count {
		display: inline-block;
		margin-left: 10px;
		border-radius: 5px;
		width: 26px;
		font-size: 10pt;
		text-align: center;
		background-color: #999999;
		color: white;
	}
	
	#funding_history .select_tab {
		background-color: rgba(0,0,0,.05);
		color: #1d85ea;
	}
	
	/* 검색창 */
	#funding_history .search {
		float: right;
		text-align: right;
	}
	#funding_history .search input {
		border: 1px solid lightgray;
		border-radius: 5px;
		width: 350px;
		padding: 7px 7px 7px 12px;
	}
	
	/* 총 n건의 후원이 있습니다. */
	#funding_history div.list_count {
		color: #545454;
		background-color: #eeeded;
		padding: 15px 15px 15px 30px;
		margin-bottom: 17px;
		border-radius: 5px;
	}
	#funding_history div.list_count i {margin-right: 15px;}
	
	
	/* 후원한 프로젝트 목록 */
	#funding_history .funding_list {
		display: none;
	}
	#funding_history .funding_list a, .funding_list a:hover {
		text-decoration: none;
		color: #4a4a4a;
	}
	#funding_history .funding_list table {
		border-collapse: separate;
		margin-bottom: 15px;
		border: 1px solid lightgray;
		border-radius: 8px;
		background-color: white;
		width: 100%;
		font-size: 10.5pt;
	}
	#funding_history .funding_list table td {
		padding: 10px;
	}
	#funding_history .funding_list table tr:first-child td {
		border-bottom: 1px solid rgba(10,10,10,0.05);
	}
	#funding_history .funding_list table td:last-child {
		width: 150px; text-align: right;
	}
	#funding_history img#project_main_img {
		width: 150px;
	}
	#funding_history td.funding_image {
		text-align: center;
		width: 180px;
	}
	#funding_history td.funding_detail {
		font-size: 10pt;
	}
	#funding_history td.funding_detail * {margin: 0px; line-height: 1.8;}
	#funding_history td.funding_detail .project_status {
		width: 65px;
		padding: 2px 0px 2px 0px;
		margin-bottom: 5px;
		border: 1px solid #1d85ea; color: #1d85ea;
		border-radius: 3px;
		font-size: 8pt;
		font-weight: bold;
		text-align: center;
	}
	#funding_history td.funding_detail .project_title,
	#funding_history td.funding_detail .funding_price {
		font-size: 10pt;
	}
	#funding_history td.funding_detail .creator_name {
		margin-left: 7px;
		font-size: 8pt;
	}
	#funding_history td.funding_detail .payment_status {
		color: #1d85ea;
	}
	
	
	/* 후원이 없습니다. */
	#funding_history .list_none {
		padding: 120px 0px 120px 0px;
		text-align: center;
		font-size: 14pt;
		font-weight: bold;
		color: #a7a7a7;
		opacity: 0.5;
	}
	#funding_history .list_none i {
		font-size: 40pt;
		margin-bottom: 15px;
	}

