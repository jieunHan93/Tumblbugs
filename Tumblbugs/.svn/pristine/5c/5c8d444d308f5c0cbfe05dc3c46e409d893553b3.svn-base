/**
	funding_history(내 후원현황) -----------------------------------------------------------------------------------------------------
	조수윤 --------------------------------------------------------------------------------------------------------------------------
**/
	/* 페이지 상단부 - 프로필 이미지, OOO님의 후원 현황 */
	#funding_history section.page_title {
		text-align: center;
		padding: 50px 0px 50px 0px;
	}
	#funding_history section.page_title span {
		display: inline-block;
		width: 105px; height: 105px;
		border-radius: 105px;
		overflow: hidden;
	}
	#funding_history section.page_title img {
		width: 105px;
	}
	#funding_history section.page_title div {
		padding: 10px 0px 10px 0px;
		font-size: 13.5pt;
		font-weight: bold;
	}
	
	/**/
	#funding_history section.page_content {
		background-color: #f6f5f5;
		min-height: 680px;
	}
	#funding_history article {
		width: 1080px;
		margin: auto;
	}
	#funding_history li {list-style-type: none;}
	
	
	/* 탭 */
	#funding_history article#page_content_tab {
		padding: 20px 0px 20px 0px;
	}
	#funding_history #page_content_tab nav,
	#funding_history #page_content_tab li,
	#funding_history .search {
		display: inline-block;
		font-size: 10.5pt;
	}
	#funding_history #page_content_tab li {
		border-radius: 4px;
		padding: 8px 10px 8px 10px;
		margin-right: 8px;
		cursor: pointer;
		/* background-color: #eeeded; */
	}
	#funding_history #page_content_tab li:hover {
		background-color: rgba(0,0,0,.05);
	}
	#funding_history #page_content_tab li span#list_count {
		display: inline-block;
		margin-left: 10px;
		border-radius: 5px;
		width: 26px;
		font-size: 10pt;
		text-align: center;
		background-color: #999999;
		color: white;
	}
	
	#funding_history .select_tab {
		background-color: rgba(0,0,0,.05);
		color: #1d85ea;
	}
	
	/* 검색창 */
	#funding_history .search {
		float: right;
		text-align: right;
	}
	#funding_history .search input {
		border: 1px solid lightgray;
		border-radius: 5px;
		width: 350px;
		padding: 7px 7px 7px 12px;
	}
	
	/* 총 n건의 후원이 있습니다. */
	#funding_history div.list_count {
		color: #545454;
		background-color: #eeeded;
		padding: 15px 15px 15px 30px;
		margin-bottom: 17px;
		border-radius: 5px;
	}
	#funding_history div.list_count i {margin-right: 15px;}
	
	
	/* 후원한 프로젝트 목록 */
	#funding_history .funding_list {
		display: none;
	}
	#funding_history .funding_list a, .funding_list a:hover {
		text-decoration: none;
		color: #4a4a4a;
	}
	#funding_history .funding_list table {
		border-collapse: separate;
		margin-bottom: 15px;
		border: 1px solid lightgray;
		border-radius: 8px;
		background-color: white;
		width: 100%;
		font-size: 10.5pt;
	}
	#funding_history .funding_list table td {
		padding: 10px;
	}
	#funding_history .funding_list table tr:first-child td {
		border-bottom: 1px solid rgba(10,10,10,0.05);
	}
	#funding_history .funding_list table td:last-child {
		width: 150px; text-align: right;
	}
	#funding_history img#project_main_img {
		width: 150px;
	}
	#funding_history td.funding_image {
		text-align: center;
		width: 180px;
	}
	#funding_history td.funding_detail {
		font-size: 10pt;
	}
	#funding_history td.funding_detail * {margin: 0px; line-height: 1.8;}
	#funding_history td.funding_detail .project_status {
		width: 65px;
		padding: 2px 0px 2px 0px;
		margin-bottom: 5px;
		border: 1px solid #1d85ea; color: #1d85ea;
		border-radius: 3px;
		font-size: 8pt;
		font-weight: bold;
		text-align: center;
	}
	#funding_history td.funding_detail .project_title,
	#funding_history td.funding_detail .funding_price {
		font-size: 10pt;
	}
	#funding_history td.funding_detail .creator_name {
		margin-left: 7px;
		font-size: 8pt;
	}
	#funding_history td.funding_detail .payment_status {
		color: #1d85ea;
	}
	
	
	/* 후원이 없습니다. */
	#funding_history .list_none {
		padding: 120px 0px 120px 0px;
		text-align: center;
		font-size: 14pt;
		font-weight: bold;
		color: #a7a7a7;
		opacity: 0.5;
	}
	#funding_history .list_none i {
		font-size: 40pt;
		margin-bottom: 15px;
	}

