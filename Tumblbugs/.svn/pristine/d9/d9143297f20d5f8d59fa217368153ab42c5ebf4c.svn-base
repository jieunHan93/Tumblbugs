package com.tumblbugs.vo;

public class ItemVO {
	String item_id, gift_id, item_title, item_option_type, item_option, item_check;
	int item_count, rno;
	
	
	public int getRno() {
		return rno;
	}
	public void setRno(int rno) {
		this.rno = rno;
	}
	public String getItem_id() {
		return item_id;
	}
	public void setItem_id(String item_id) {
		this.item_id = item_id;
	}
	public String getGift_id() {
		return gift_id;
	}
	public void setGift_id(String gift_id) {
		this.gift_id = gift_id;
	}
	public String getItem_title() {
		return item_title;
	}
	public void setItem_title(String item_title) {
		this.item_title = item_title;
	}
	public String getItem_option_type() {
		return item_option_type;
	}
	public void setItem_option_type(String item_option_type) {
		this.item_option_type = item_option_type;
	}
	public String getItem_option() {
		return item_option;
	}
	public void setItem_option(String item_option) {
		this.item_option = item_option;
	}
	public String getItem_check() {
		return item_check;
	}
	public void setItem_check(String item_check) {
		this.item_check = item_check;
	}
	public int getItem_count() {
		return item_count;
	}
	public void setItem_count(int item_count) {
		this.item_count = item_count;
	}
	
	
}
